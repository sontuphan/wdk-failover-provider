{
  "name": "@tetherto/wdk-failover-provider",
  "version": "1.0.0-beta.1",
  "description": "Zero-dependency failover mechanism for seamless fallback when providers break",
  "keywords": [
    "wdk",
    "wallet",
    "failover"
  ],
  "homepage": "https://github.com/tetherto/wdk-failover-provider#readme",
  "bugs": {
    "url": "https://github.com/tetherto/wdk-failover-provider/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tetherto/wdk-failover-provider.git"
  },
  "license": "Apache-2.0",
  "author": "Tether",
  "type": "module",
  "main": "src/index.js",
  "types": "./types",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./index.js"
    },
    "./package": {
      "default": "./package.json"
    }
  },
  "scripts": {
    "build:types": "tsc",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:integration": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/integration",
    "test:integration:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/integration --coverage"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "ethers": "^6.16.0",
    "jest": "^30.2.0",
    "standard": "^17.1.2",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "standard": {
    "ignore": [
      "bare.js",
      "tests/**/*.js"
    ]
  }
}
